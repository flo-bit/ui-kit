{
	"name": "fuchs",
	"version": "0.0.5",
	"type": "module",
	"description": "Beautiful UI components built with Tailwind 4 and Svelte 5",
	"homepage": "https://flo-bit.dev/ui-kit",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/flo-bit/ui-kit.git"
	},
	"author": "flo-bit (http://flo-bit.dev/)",
	"bugs": "https://github.com/flo-bit/ui-kit/issues",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"build:package": "vite build && npm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint ."
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/postcss": "^4.0.9",
		"@tailwindcss/typography": "^0.5.16",
		"@zerodevx/svelte-img": "^2.1.2",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.14.0",
		"mdsvex": "^0.12.3",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"publint": "^0.3.2",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.9",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.0.0"
	},
	"keywords": [
		"svelte",
		"ui-kit"
	],
	"peerDependencies": {
		"@sveltejs/kit": "^2.16.0",
		"svelte": "^5.0.0"
	},
	"dependencies": {
		"@atproto/api": "^0.14.7",
		"@number-flow/svelte": "^0.3.3",
		"@sveltejs/adapter-static": "^3.0.8",
		"@texel/color": "^1.1.9",
		"@thisux/sveltednd": "^0.0.20",
		"@threlte/core": "^8.0.1",
		"@threlte/extras": "^9.0.1",
		"@types/three": "^0.173.0",
		"bits-ui": "^1.3.4",
		"cheerio": "^1.0.0",
		"hls.js": "^1.5.20",
		"layerchart": "^1.0.0",
		"paneforge": "^0.0.6",
		"plyr": "^3.7.8",
		"simplex-noise": "^4.0.3",
		"svelte-sonner": "^0.3.28",
		"tailwind-variants": "^0.3.1",
		"tailwindcss-animate": "^1.0.7",
		"three": "^0.173.0",
		"vite-plugin-glsl": "^1.3.1",
		"wavesurfer.js": "^7.9.1"
	},
	"license": "MIT"
}
